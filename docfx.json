{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/*.csproj"
          ]
        }
      ],
      "dest": "docs/api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "docs/api/**.yml",
          "docs/api/index.md"
        ]
      },
      {
        "files": [
          "docs/articles/**.md",
          "docs/articles/**.yml"
        ]
      },
      {
        "files": [
          "toc.yml",
          "index.md"
        ],
        "src": "./",
        "dest": "./"
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": []
      }
    ],
    "dest": "docs/_site",
    "globalMetadataFiles": [],
    "globalMetadata":{
      "_appName": "ReFreSH.MobileSuit",
      "_appLogoPath": "images/logo_docfx.png",
      "_appFaviconPath": "images/logo.png"
    },
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern",
      "docs/templates/material"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
