{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/*.csproj"
          ]
        }
      ],
      "dest": "docs/api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/*.{md,yml}",
          "articles/**/*.{md,yml}",
          "toc.yml",
          "index.md"
        ],
        "src": "docs/"
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      },
      {
        "files": [
          "*.*"
        ],
        "src": "./docs/static"
      }
    ],
    "sitemap": {
      "baseUrl": "https://hit-refresh.github.io/MobileSuit/",
      "priority": 0.3,
      "changefreq": "monthly"
    },
    "dest": "docs/_site",
    "globalMetadataFiles": [],
    "globalMetadata": {
      "_appName": "ReFreSH.MobileSuit",
      "_appLogoPath": "images/logo_docfx.png",
      "_appFaviconPath": "images/logo.png",
      "_enableSearch": true
    },
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern",
      "docs/templates/material"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}